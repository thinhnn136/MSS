@model IEnumerable<MSS_DEMO.Models.Semester>

@{
    ViewBag.Title = "Index";
}
<br/>
<h2>@Html.ActionLink("Setup the beginning of the semester", "Index", "SetupBeginSemester")</h2>
<hr />
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Semester_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Start_Date)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.End_Date)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Semester_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Start_Date)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.End_Date)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Semester_ID }) |
            @Html.ActionLink("Details", "Details", new { id=item.Semester_ID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Semester_ID })
        </td>
    </tr>
}

</table>
